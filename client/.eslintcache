tListener(\"resize\", () => setWidth(window.innerWidth));\n  }, []);\n\n  const renderToolTip = (props) => {\n    const { payload } = props;\n    let info =\n      switchPosition === \"Liquidity\"\n        ? formatNumber(payload?.[0]?.payload?.liquidity)\n        : formatNumber(payload?.[0]?.payload?.volume);\n    return (\n      <div className={classes.custom_tooltip}>\n        <p className={classes.custom_tooltip__line}>\n          {`${payload?.[0]?.payload?.day ?? \"\"}  ${\n            payload?.[0]?.payload?.month ?? \"\"\n          } ${payload?.[0]?.payload?.year ?? \"\"}`}\n        </p>\n\n        <p\n          className={classes.custom_tooltip__line}\n        >{`${switchPosition}:  ${info}`}</p>\n      </div>\n    );\n  };\n\n  return (\n    <ResponsiveContainer\n      width={\n        width <= breakpoint_mobile\n          ? \"100%\"\n          : width <= breakpoint\n          ? \"81%\"\n          : \"90%\"\n      }\n      height={150}\n    >\n      {isLoading ? (\n        <Loader />\n      ) : (\n        <BarChart\n          data={chartData}\n          margin={{\n            top: 5,\n            right: 35,\n            left: 0,\n            bottom: 10,\n          }}\n        >\n          <CartesianGrid\n            stroke=\"rgba(254, 255, 254, 0.2)\"\n            strokeDasharray=\"3 3\"\n            vertical={false}\n          />\n          <XAxis\n            dataKey=\"day\"\n            tickMargin={12}\n            axisLine={false}\n            style={{ fontSize: \"12px\" }}\n            tickLine={false}\n          >\n            <Label\n              value={`${\n                chartData.length > 0 && chartData[chartData.length - 1].month\n              }`}\n              style={{ fill: \"rgba(255, 255, 255, .5)\", fontSize: \"12px\" }}\n              offset={5}\n              position=\"right\"\n            />\n          </XAxis>\n          <Tooltip\n            content={renderToolTip}\n            cursor={{ fill: \"rgba(255, 255, 255, 0.2)\" }}\n       